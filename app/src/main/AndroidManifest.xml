        <activity android:name=".pcelar" />
        <activity android:name=".shopping" />
        <activity android:name=".poruke" />
        <activity android:name=".kupovinaMeda" />
        <activity android:name=".Istorija" />
        <activity android:name=".SviPcelari" />

        <meta-data
            android:name="com.google.android.actions"
            android:resource="@xml/my_button_bg" />

        <activity android:name=".Menu" />
        <activity android:name=".registracija" />
        <activity android:name=".MainActivity">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />

                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
    </application>

</manifest>